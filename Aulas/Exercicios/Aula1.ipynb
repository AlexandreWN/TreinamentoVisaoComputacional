     ]
    }
   ],
   "source": [
    "import numpy as np\n",
    "\n",
    "array = np.arange(16)\n",
    "arrayImpar = (array[array %2 != 0])\n",
    "\n",
    "print(array)\n",
    "print(arrayImpar)"
   ]
  }
